{
  "name": "chai-jq",
  "version": "0.0.9",
  "description": "An alternate jQuery assertion library for Chai.",
  "main": "chai-jq.js",
  "dependencies": {},
  "devDependencies": {
    "chai": "^3.4.1",
    "coveralls": "^2.11.4",
    "eslint": "^1.7.3",
    "eslint-config-defaults": "^7.0.1",
    "eslint-plugin-filenames": "^0.1.2",
    "eslint-plugin-react": "^3.6.2",
    "gulp": "3.5.2",
    "gulp-mdox": "0.0.2",
    "http-server": "^0.8.5",
    "istanbul": "^0.4.0",
    "jade": "^1.11.0",
    "jquery": "^1.11.3",
    "jsdom": "0.8.8",
    "jstransformer-marked": "^1.0.1",
    "karma": "^0.13.9",
    "karma-chrome-launcher": "^0.2.0",
    "karma-coverage": "^0.5.2",
    "karma-firefox-launcher": "^0.1.6",
    "karma-ie-launcher": "^0.2.0",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-phantomjs-shim": "^1.1.1",
    "karma-safari-launcher": "^0.1.1",
    "karma-sauce-launcher": "^0.2.14",
    "karma-spec-reporter": "0.0.20",
    "marked": "0.2.10",
    "mocha": "^2.2.5",
    "phantomjs": "^1.9.18",
    "sinon": "^1.16.1"
  },
  "scripts": {
    "lint-client": "eslint -c .eslintrc-client chai-jq.js",
    "lint-client-test": "eslint -c .eslintrc-client-test test/js/spec test/adapters/karma.js",
    "lint-server": "eslint -c .eslintrc-server *.js",
    "lint-server-test": "eslint -c .eslintrc-server-test test/js/spec test/adapters/node.js",
    "lint": "npm run lint-client && npm run lint-client-test && npm run lint-server && npm run lint-server-test",
    "test-frontend": "node node_modules/karma/bin/karma start karma.conf.js",
    "test-frontend-ci": "node node_modules/karma/bin/karma start --browsers PhantomJS,Firefox karma.conf.coverage.js",
    "test-frontend-sauce": "node node_modules/karma/bin/karma start karma.conf.saucelabs.js",
    "test-frontend-cov": "node node_modules/karma/bin/karma start karma.conf.coverage.js",
    "test-server": "mocha test/adapters/node.js",
    "test-server-cov": "istanbul cover --config .istanbul.server.yml _mocha -- test/adapters/node.js",
    "test": "npm run test-frontend && npm run test-server",
    "test-ci": "npm run test-frontend-ci && npm run test-server-cov",
    "test-cov": "npm run test-frontend-cov && npm run test-server-cov",
    "check": "npm run lint && npm run test",
    "check-ci": "npm run lint && npm run test-ci",
    "check-cov": "npm run lint && npm run test-cov",
    "server-test": "http-server -p 3030 .",
    "build-templates": "jade -P -H -o . _templates",
    "build": "npm run build-templates"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FormidableLabs/chai-jq"
  },
  "keywords": [
    "test",
    "assertion",
    "assert",
    "chai",
    "chai-plugin",
    "jquery",
    "browser",
    "vendor",
    "dom"
  ],
  "author": "Ryan Roemer <ryan.roemer@formidable.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FormidableLabs/chai-jq/issues"
  }
}
